{
  "name": "hls-video-s3-proxy",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "type": "module",
  "scripts": {
    "dev": "npx wrangler dev",
    "prepare-env": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' ./prepare-env.ts ",
    "publish": "npm run prepare-env && npx wrangler deploy",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "test": "npm run prepare-env && npm run _test",
    "_test": "NODE_OPTIONS='$NODE_OPTIONS --experimental-vm-modules' npx jest --config=jest.config.ts"
  },
  "author": "Ronan LE MEILLAT",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@aws-sdk/s3-request-presigner": "^3.529.1",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240222.0",
    "@jest/transform": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "axios": "^1.6.7",
    "child_process": "^1.0.2",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-typescript": "^3.0.0",
    "gitlog": "^4.0.8",
    "jest": "^29.7.0",
    "jest-retries": "^1.0.1",
    "m3u8-parser": "^7.1.0",
    "miniflare": "^3.20240304.0",
    "prettier": "^3.2.5",
    "tree-kill": "^1.2.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "wrangler": "^3.32.0"
  }
}
